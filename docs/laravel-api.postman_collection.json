	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"// jeśli nie mamy tokenu, to dociągnij go z /sanctum/csrf-cookie",
					"if (!pm.environment.get('X_XSRF_TOKEN')) {",
					"    pm.sendRequest({",
					"        url: pm.environment.get('API_BASE_URL') + '/sanctum/csrf-cookie',",
					"        method: 'GET'",
					"    }, function (err, res) {",
					"        if (err) {",
					"            console.log('CSRF error:', err);",
					"            return;",
					"        }",
					"",
					"        const raw = res.cookies.get('XSRF-TOKEN');",
					"        if (raw) {",
					"            pm.environment.set('X_XSRF_TOKEN', decodeURIComponent(raw));",
					"        }",
					"    });",
					"}"
				]
			}
		},
